# Description : Session management in a clean and simple way
# URL         : http://www.brain-dump.org/projects/abduco
# Maintainer  : camille, camille@laserbat.pw
# Depends on  :

name=abduco
version=0.6
release=2
source=("http://www.brain-dump.org/projects/$name/$name-$version.tar.gz")

build() {
    cd "$name-$version"

    make PREFIX=/usr DESTDIR="$PKG" install
}

