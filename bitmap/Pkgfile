# Description : X bitmap (XBM) editor and converter utilities
# URL         : http://www.x.org
# Maintainer  : camille, camille at laserbat dot pw
# Depends on  : xorg-libxaw xorg-xbitmaps

name=bitmap
version=1.0.9
release=1
source=("https://www.x.org/archive/individual/app/$name-$version.tar.gz")

build() {
    cd "$name-$version"

    ./configure --prefix=/usr

    make DESTDIR="$PKG" install
}
